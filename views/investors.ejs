<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='../stylesheets/style.css' />

  </head>
  <body>


<h1>Investors List</h1>

<table class="responstable">
  
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Year</th>
  </tr>

  <%
  
   data.forEach(function(d) { %>
    <tr>
      <td><%= d.first %></td>
      <td><%= d.last %></td>
      <td><%= d.year %></td>
    </tr>    
  <% }) %>
</table>



  </body>
</html>